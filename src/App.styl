a
	text-decoration: none;
	color: inherit;
ul
	padding: 0;
	-webkit-margin-before: 0;
	-webkit-margin-after: 0;
	-webkit-margin-start: 0;
	-webkit-margin-end: 0;
	-webkit-padding-start: 0;
body
	margin: 0;
	font-family: 'Raleway', sans-serif;

.center
	display: flex;
	align-items: center;
	justify-content: center;

.cta
	width: 280px;
	height: 50px;
	line-height: 50px;
	border: 1px solid #fff;
	border-radius: 50px;
	font-size: 28px;

.container
	display: flex
	flex-direction: column
	align-items: center
	width: 100%;